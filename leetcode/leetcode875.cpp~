#include<bits/stdc++.h>
using namespace std;

void printV(vector<int> v){
  for(int i:v){
    cout<<i<<' ';
  }
  cout<<'\n';
}

void printM(vector<vector<int>> m){
  for(vector<int> v:m){
    printV(v);
  }
}

int minEatingSpeed(vector<int> &p, int h)
{
  int n = p.size();
  int k=4,h=0;
  for(int i=0;i<n;i++){
    while(p[i]>0){
      h++;
      p[i]-=k;
    }
  }
}

int main()
{
  return 0;
}
