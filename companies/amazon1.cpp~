#include<bits/stdc++.h>
using namespace std;

void printV(vector<int> v)
{
  for(auto i:v){
    cout<<i<<' ';
  }
  cout<<'\n';
}

void printM(vector<vector<int>> m)
{
  for(auto v:m){
    printV(v);
  }
}

long sol(vector<int> p, int a)
{
  int n = p.size();
}

int main()
{
  vector<int> p = {1,2,6,7};
  int a = 5;
  cout<<sol(p,a);
  return 0;
}
